          <ProjectItem ReplaceParameters="true">app.config</ProjectItem>
          <ProjectItem ReplaceParameters="true" TargetFileName="Client.WPF.Global.tt">Client.WPF.Global.tt</ProjectItem>
          <ProjectItem ReplaceParameters="true" TargetFileName="Client.WPF.tt">Client.WPF.tt</ProjectItem>
