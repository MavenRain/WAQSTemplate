        </Folder>
      </Folder>
      <ProjectItem ReplaceParameters="true">MainViewModel.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true">WAQSModel.edmx</ProjectItem>
      <ProjectItem RepalceParameters="true">WAQSModel.edmx.diagram</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>