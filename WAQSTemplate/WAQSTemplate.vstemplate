        <ProjectItem ReplaceParameters="true" TargetFileName="Server.svc">Server.svc</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Server.tt">Server.tt</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="Server.waqs">Server.waqs</ProjectItem>
